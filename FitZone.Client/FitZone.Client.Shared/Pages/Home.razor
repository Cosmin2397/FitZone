@page "/"
@using FitZone.Client.Shared.Services
@using FitZone.Client.Shared.Services.Interfaces
@inject IFormFactor FormFactor

<PageTitle>Home</PageTitle>
@if(UserState.Instance.IsAuthentificated)
{
    if(UserState.Instance.GetRole == "Client")
    {
        <HomeClient></HomeClient>
    }
    else if (UserState.Instance.GetRole == "Employee")
    {}
    else if (UserState.Instance.GetRole == "PersonalTrainer")
    { }
    else if (UserState.Instance.GetRole == "GymManager")
    { }
    else if (UserState.Instance.GetRole == "AppManager")
    { }
    else
    {
        <Login></Login>
    }
}
else
{
    <Login></Login>
}


